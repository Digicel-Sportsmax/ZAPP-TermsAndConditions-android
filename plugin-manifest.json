{
  "api": {
    "require_startup_execution": true,
    "class_name": "com.sportsmax.termsandconditions_android.StartUpAdapter",
    "proguard_rules": "##---------------Proguard configuration for termsAndConditions and dataConsent plugin  ----------\n-ignorewarnings\n-dontshrink\n-keepclassmembers class com.sportsmax.termsandconditions_android.**\n-keepclassmembers class com.sportsmax.termsandconditions_android.** { *; }",

    "modules": []
  },
  "dependency_repository_url": [

{
      "url": "https://dl.bintray.com/swrve-inc/android"
    },
    {
      "url": "https://github.com/Digicel-Sportsmax/ZAPP-TermsAndConditions-android.git"
    }
  ],
  "platform": "android",
  "author_name": "Elias bou nader",
  "author_email": "elias.bounader@gotocme.com",
  "manifest_version": "0.0.41",
  "name": "[Sportsmax]: Terms & Conditions",
  "description": "Sportsmax Terms & Conditions",
  "type": "general",
  "identifier": "SMaxTermsAndCondition",
  "ui_builder_support": true,
   "screen": true,
   "preload": true,
  "dependency_name": "com.applicaster:TermsAndConditionsPluginAndroid",
  "dependency_version": "0.0.41",
  "whitelisted_account_ids": [
    "5af949534da8f300080050f9"
  ],
  "min_zapp_sdk": "11.2.0",
  "deprecated_since_zapp_sdk": "",
  "unsupported_since_zapp_sdk": "",
  "react_native": false,
  "custom_configuration_fields": [
    {
      "type": "checkbox",
      "key": "show_screen_at_launch",
      "tooltip_text": "Flag to indicate if to present the screen on app launch"
    },
    {
      "type": "checkbox",
      "key": "show_data_consent_warning",
      "tooltip_text": "Flag to indicate if to display data consent warning after terms and conditions is accepted."
    },
    {
      "type": "uploader",
      "key": "navigation_header_image",
      "tooltip_text": "image to be shown in the header"
    },
    {
      "type": "text_input",
      "key": "navigation_header_text",
      "tooltip_text": "text to be shown in the header if no image is uploaded"
    },
    {
      "type": "text_input",
      "key": "navigation_header_background",
      "tooltip_text": "Navigation Header background Color",
      "initial_value": "#356bac"
    },
    {
      "type": "textarea",
      "key": "terms_condition_text",
      "tooltip_text": "terms and condition text"
    },
    {
      "type": "textarea",
      "key": "data_consent_text",
      "tooltip_text": "Data Consent text"
    },
    {
      "type": "text_input",
      "key": "next_button_text",
      "tooltip_text": "Next Button Text"
    },
    {
      "type": "text_input",
      "key": "agree_button_text",
      "tooltip_text": "Agree Button Text"
    },
    {
      "type": "text_input",
      "key": "agree_button_text_color",
      "tooltip_text": "Agree / Next Button Text Color",
      "initial_value": "#FFFFFF"
    },
    {
      "type": "text_input",
      "key": "agree_button_bg_color",
      "tooltip_text": "Agree / Next Button background Color",
      "initial_value": "#356bac"
    },
    {
      "type": "text_input",
      "key": "terms_and_condition_checkbox",
      "tooltip_text": "terms and condition checkbox"
    },
    {
      "type": "text_input",
      "key": "data_consent_checkbox",
      "tooltip_text": "data consent checkbox"
    }
  ],
 "targets": [
    "mobile"
  ]
}
